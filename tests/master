library(largeScaleR)
userProcess(host="localhost")
commsProcess(host="localhost", port=6379L, execute=FALSE)
logProcess(host="localhost", port=5140L, execute=FALSE)
init()

x <- chunkRef(1)
host(x) <- "127.0.0.1"
port(x) <- "12345"
size(x) <- 150
from(x) <- 1
to(x) <- 150
y <- distObjRef(list(x))

# sum(do.dcall("object.size", list(y)))

z <- do.dcall("biglm::biglm", list(I(Sepal.Length ~ Sepal.Width), y))
emerge(z)
